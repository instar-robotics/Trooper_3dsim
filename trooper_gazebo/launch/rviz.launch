<?xml version="1.0"?>
<launch>

	 <arg name="trooper" default="$(find trooper_description)/urdf/trooper.urdf"/>
	 <arg name="gui" default="true" />

	 <arg name="gpu" default="false"/>
	 <arg name="organize_cloud" default="false"/>
	 <param name="robot_description" command="$(find xacro)/xacro --inorder '$(find trooper_description)/urdf/trooper.urdf.xacro' organize_cloud:=$(arg organize_cloud) gpu:=$(arg gpu)" />


	 <arg name="rvizconfig" default="$(find urdf_tutorial)/rviz/urdf.rviz" />

	 <param name="robot_description" textfile="$(find trooper_description)/urdf/trooper.urdf" />
	<!-- <param name="use_gui" value="$(arg gui)"/> -->

	 <node name="joint_state_publisher_gui" pkg="joint_state_publisher_gui" type="joint_state_publisher_gui" />
	 <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" />
	 <node name="rviz" pkg="rviz" type="rviz" args="-d $(arg rvizconfig)" required="true" />


</launch>
